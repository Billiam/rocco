<!DOCTYPE html>
<html>
<head>
  <title>{{ title }}</title>
  <meta http-equiv="content-type" content="text/html;charset=utf-8">
  <meta name="viewport" content="width=device-width, target-densitydpi=160dpi, initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
  {{#stylesheet}}
    <link rel="stylesheet" media="all" href="{{ . }}">
  {{/stylesheet}}
</head>
<body>
  <div id='container'>
    <div id="background"></div>
    {{#sources?}}
      <ul id="jump_to">
      <li>
        <a class="large" href="javascript:void(0);">Jump To &hellip;</a>
        <a class="small" href="javascript:void(0);">+</a>
        <div id="jump_wrapper">
          <div id="jump_page">
            {{#filter?}}
              <div id="jump_actions">
               <input id="jump_filter" placeholder="Filter Files &hellip;">
              </div>
            {{/filter?}}
            <div id="jump_sources" class="jump_page">
              {{#sources}}
                <a class="source" href="{{ url }}">{{ basename }}</a>
              {{/sources}}
            </div>
            <div id="jump_results" class="jump_page"></div>
          </div>
        </div>
      </li>
    </ul>
  {{/sources?}}
  <ul class="sections">
    <li id="title">
      <div class="annotation">
        <h1>{{ title }}</h1>
      </div>
    </li>
    {{#sections}}
    <li id="section-{{ section_id }}">
      <div class="annotation">
        <div class="pilwrap">
          <a class="pilcrow" href="#section-{{ section_id }}">&#182;</a>
        </div>
        {{{ docs }}}
      </div>
      <div class="content">
        <div class='highlight'><pre>{{{ code }}}</pre></div>
      </div>
    </li>
    {{/sections}}
  </ul>
  </div>
  {{#sources?}}
    <script type="text/javascript" src="http://billiam.github.com/rocco/resources/docco.js"></script>
  {{/sources?}}
</body>
</html>